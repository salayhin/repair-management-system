<div id="inSlider" class="carousel carousel-fade" data-ride="carousel">
  <ol class="carousel-indicators">
    <li data-target="#inSlider" data-slide-to="0" class="active"></li>
    <li data-target="#inSlider" data-slide-to="1"></li>
  </ol>
  <div class="carousel-inner" role="listbox">
    <div class="item active">
      <div class="container">
        <div class="carousel-caption">
          <h1>We repair<br/>
            mobiles, tablets,<br/>
            and sell mobile accessories
            </h1>
          <p>We've service centers all over the country.</p>
          <!--<p>-->
            <!--<a class="btn btn-lg btn-primary" href="#" role="button">READ MORE</a>-->
            <!--<a class="caption-link" href="#" role="button">Inspinia Theme</a>-->
          <!--</p>-->
        </div>
        <div class="carousel-image wow zoomIn">
          <%= image_tag 'landing/mobile-doctor.jpg' %>
        </div>
      </div>
      <!-- Set background for slide in css -->
      <div class="header-back one"></div>

    </div>
    <div class="item">
      <div class="container">
        <div class="carousel-caption blank">
          <h1>We repair smart phones <br/> bar and touch phones.</h1>
          <p>Kingstar Mobile inherited that same expert and experienced, dedicated and dynamic team to launch its own brand</p>
          <!--<p><a class="btn btn-lg btn-primary" href="#" role="button">Learn more</a></p>-->
        </div>
      </div>
      <!-- Set background for slide in css -->
      <div class="header-back two"></div>
    </div>
  </div>
  <a class="left carousel-control" href="<%= root_path %>#inSlider" role="button" data-slide="prev">
    <span class="fa fa-arrow-left" aria-hidden="true"></span>
    <span class="sr-only">Previous</span>
  </a>
  <a class="right carousel-control" href="<%= root_path %>#inSlider" role="button" data-slide="next">
    <span class="fa fa-arrow-right" aria-hidden="true"></span>
    <span class="sr-only">Next</span>
  </a>
</div>

